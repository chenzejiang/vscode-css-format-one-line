{
	"name": "css-format-one-line",
	"displayName": "css-format-one-line",
	"description": "css格式化为单行",
	"version": "1.0.0",
	"publisher":"css-format-one-line",
	"icon": "images/logo128.png",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
        "keybindings": [
            {
                "command": "extension.helloWorld",
                "key": "ctrl+j",
                "mac": "cmd+j",
                "when": "editorTextFocus"
			}
        ]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/chenzejiang/vscode-css-format-one-line"
    },
    "homepage": "https://github.com/chenzejiang/vscode-css-format-one-line/blob/master/README.md"
}
